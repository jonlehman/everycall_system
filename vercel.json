{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "framework": "nextjs",
  "installCommand": "corepack pnpm install --frozen-lockfile --prod=false",
  "buildCommand": "corepack pnpm --filter @everycall/web build",
  "rewrites": [
    {
      "source": "/v1/(.*)",
      "destination": "/api/v1/$1"
    }
  ]
}
